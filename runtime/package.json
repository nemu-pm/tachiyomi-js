{
  "name": "@tachiyomi-js/runtime",
  "version": "0.1.0",
  "description": "Runtime for loading and executing Tachiyomi extensions compiled to JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemu-pm/tachiyomi-js.git",
    "directory": "runtime"
  },
  "homepage": "https://github.com/nemu-pm/tachiyomi-js#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "bun": {
        "types": "./dist/index.node.d.ts",
        "default": "./dist/index.node.js"
      },
      "node": {
        "types": "./dist/index.node.d.ts",
        "default": "./dist/index.node.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tachiyomi",
    "manga",
    "extension",
    "runtime"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "canvas": "^3.0.0-rc3"
  },
  "peerDependencies": {
    "canvas": "^3.0.0-rc3"
  },
  "peerDependenciesMeta": {
    "canvas": {
      "optional": true
    }
  }
}
